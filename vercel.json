{
  "version": 2,
  "builds": [
      {
          "src": "app.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/signin",
          "dest": "/app.js"
      },
      {
          "src": "/signup",
          "dest": "/app.js"
      },
      {
        "src": "/time",
        "dest": "/app.js"
      }
  ]
}